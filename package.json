  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4300 --proxy-config ./e2e/proxy.config.json",
    "start.mocking": "node ./e2e/server.js",
    "build": "ng build --prod",
    "e2e": "protractor e2e/config/protractor.conf.js",
    "postinstall": "npm run webdriver.update",
